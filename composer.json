{
  "name": "golding/dns-zone-configurator",
  "license": "MIT",
  "type": "library",
  "description": "Php library for parsing and editing dns zones files programmatically with high level abstraction.",
  "keywords": [
    "dns",
    "zone file"
  ],
  "authors": [
    {
      "name": "Viskov Sergey",
      "homepage": "https://github.com/voksiv"
    },
    {
      "name": "Marco Grob",
      "homepage": "https://github.com/mwbgrob"
    }
  ],
  "require": {
    "php": ">=8.0",
    "marc-mabe/php-enum" : ">=2.2,<5",
    "laminas/laminas-validator" : "~2.14",
    "golding/dns-zone-tokenizer": "~v1"
  },
  "require-dev": {
    "theseer/phpdox": "~0.8",
    "phpunit/phpunit": "~8.1",
    "tideways/profiler": "~2.0"
  },
  "autoload": {
    "psr-0": {
      "Golding\\dns": "src"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Golding\\dev": "src"
    }
  }
}
